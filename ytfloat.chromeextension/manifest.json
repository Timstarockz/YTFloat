{
  "manifest_version": 2,

  "name": "YTFloat",
  "description": "Minimize your YT videos onto your desktop. Multi-Tasking FTW!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
    
    "background": {
        "scripts": ["background.js"]
    },
    
  "permissions": [
    "tabs"
  ]
}
